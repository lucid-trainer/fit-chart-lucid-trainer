google.charts.load("current", {"packages":["corechart"]});
google.charts.setOnLoadCallback(drawArea);

const awake = "area { color: #FFAC1C }";
const lightp = "area { color: #5D3FD3 ; opacity: 0.5 }";
const remp0 = "area { color: #4AB9DE ; opacity: 0.25 }";
const remp1 = "area { color: #4AC5DE ; opacity: 0.4 }";
const remp2 = "area { color: #4ADED2 ; opacity: 0.6 }";
const remp3 = "area { color: #4ADEC5 ; opacity: 0.75 }";
const remp4 = "area { color: #4ADEB9 ; opacity: 1 }";

function drawArea() {
    var data = new google.visualization.DataTable();
    data.addColumn('date', 'time');
    data.addColumn('number', 'stage');
    data.addColumn({type: 'string', role: 'style'});
    data.addRows([
{appEventData}
    ]);

    // Set chart options
    var options = {
        width: 1400,
        height: 200,
        areaOpacity: .8,
        lineWidth: 0,
        seriesType: 'area',
        backgroundColor: 'transparent',
        legend: {position: 'none'},
        vAxis: {
            gridlines: {
              color: 'none',
            },
            viewWindow: {
              min: 0,
              max: 10
            },
            textPosition: 'none'
        },
        hAxis: {
            gridlines: {
                color: 'none',
            },
            textPosition: 'none'
        }
    };

    // Instantiate and draw our charts, passing in some options.
    for (let i = 1; i <= 6; i++) {
        var elementId = "mid-" + i;
        var container = document.getElementById(elementId);
        var chart = new google.visualization.ComboChart(container);
        chart.draw(data, options);    
    }   
}